{
  "version": 2,
  "builds": [
    {
      "src": "food-product-explorer/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "installCommand": "cd food-product-explorer && npm install",
        "buildCommand": "cd food-product-explorer && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
